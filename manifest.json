{
  "manifest_version": 2,
  "name": "Beautify JSON",
  "short_name": "beautify_json",
  "description": "Validates and formats JSON texts",
  "homepage_url": "http://obedtandadjaja.github.io",
  "version": "0.2",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["src/inject/inject.js"]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["src/inject/inject.css"]
    }
  ]
}
